run = "python app.py"
language = "python"
onBoot = "python -m flask --version"
hidden = ["*.pyc", "__pycache__", ".pytest_cache"]

[env]
FLASK_APP = "app.py"
FLASK_ENV = "development"

[packager]
language = "python"
ignoredPackages = ["unity", "node_modules"]

[packager.features]
enabledForHosting = true